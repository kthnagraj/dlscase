CREATE OR REPLACE VIEW iv_voxtron_contact_erweitert_skill_and_queue_isfddublette FOLDER = '/voxtron/02_integration/contact'
    PRIMARY KEY ( 'contact_contactid' , 'skillid' ) AS SELECT iv_voxtron_contact_erweitert_skill_and_queue.contact_contactid AS contact_contactid, iv_voxtron_contact_erweitert_skill_and_queue.skillid AS skillid, iv_voxtron_contact_erweitert_skill_and_queue.skill_name AS skill_name, true AS is_fd_dublette FROM iv_voxtron_contact_erweitert_skill_and_queue AS iv_voxtron_contact_erweitert_skill_and_queue INNER JOIN iv_voxtron_contact_erweitert_skill_and_queue AS iv_voxtron_contact_erweitert_skill_and_queue1 ON iv_voxtron_contact_erweitert_skill_and_queue.contact_contactid = iv_voxtron_contact_erweitert_skill_and_queue1.contact_contactid  WHERE (iv_voxtron_contact_erweitert_skill_and_queue.skill_name like 'FD-%' AND (iv_voxtron_contact_erweitert_skill_and_queue1.skill_name like 'FDB%' OR iv_voxtron_contact_erweitert_skill_and_queue1.skill_name like 'FDS%' OR iv_voxtron_contact_erweitert_skill_and_queue1.skill_name like 'FDF%' OR iv_voxtron_contact_erweitert_skill_and_queue1.skill_name like 'FDD%' OR iv_voxtron_contact_erweitert_skill_and_queue1.skill_name like 'FDHH%' OR iv_voxtron_contact_erweitert_skill_and_queue1.skill_is_fdmnebenstelle is true ));

ALTER VIEW iv_voxtron_contact_erweitert_skill_and_queue_isfddublette
 LAYOUT (iv_voxtron_contact_erweitert_skill_and_queue = [100, 140, 300, 340], iv_voxtron_contact_erweitert_skill_and_queue = [550, 140, 380, 370]);

